<h1 class="airlineName">Emperor Airlines</h1>

<button id="createBtn" class="btn btn-danger createPlane">Create Plane</button>

<div id="createForm" class="createAirplane form-inline">
    <div id="newAirplane" class="form_group">
      <div class="form_group">
        <input type="text" placeholder="name" id="airplaneName" class="form-control">
        <input type="text" placeholder="rows" id="airplaneRow" class="form-control">
        <input type="text" placeholder="cols" id="airplaneCol" class="form-control">
      </div>

      <div class="buttons">
        <a href="" class="cancel">Cancel</a>
        <button class="save btn btn-danger">Save</button>
      </div>
    </div>
</div>
