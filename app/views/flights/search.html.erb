<div class="heading">
  <a href="/airplanes" class="title">Home</a>
  <a href="/search" class="title">Search</a>
  <a href="/admin" class="title">Admin</a>
  <a href="/reservations" class="title">Reservation</a>
</div>
<h1 class="airlineName">Emperor Airlines</h1>

<button id="createSearch" class="btn btn-danger createPlane">Search Flight</button>

<div id="createForm" class="createAirplane form-inline">  
  
    <div id="newAirplane" class="form_group">
      <div class="form_group">
        <input type="text" placeholder="from" id="from" class="form-control">
        <input type="text" placeholder="to" id="to" class="form-control">
        <%= render 'flights/location' %>
      </div>

      <div class="buttons">
        <a href="" class="cancel">Cancel</a>
        <button class="save btn btn-danger">Save</button>
      </div>    
    </div>
</div>

<h4>Flight Search Result</h4>



